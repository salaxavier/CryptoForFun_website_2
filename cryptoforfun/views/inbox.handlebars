
<div class="cos">
<h1 id="ciphers_font">Inbox </h1>
<div class="about">
  <p id="pic"><img src="../../images/messages.png" width="300" height="auto"></p>
  <div id="about_cipher">
    <p>Here you can check your messages!</p>
</div>
</div><br><br>
<div id="messages"></div><br>


    {{#each message_list}}
    <div class="container">
      <label>From:</label><br>
      <input readonly name="from_user" value="{{from_user}}"></input><br><br>
      <label>Cipher:</label><br>
      <input readonly name="cipher" value="{{cipher}}"></input><br><br>
      <label>Shift/Keyword:</label><br>
      <input id="shift{{_id}}" readonly name="param" value="{{param}}"></input><br><br>
      <label>Message:</label><br>
      <textarea id="message{{_id}}" readonly name="message_ciphered">{{message_ciphered}}</textarea>
      <input type="button" value="Decode" onclick ="decode('{{_id}}')"><br><br>
      <!--<input type="button" value="Decode" onclick ="decode()" href="scripts/{{cipher}}.js"><br><br>-->
      <label id="output_name">Decoded message</label><br>
      <textarea id="output{{_id}}" readonly></textarea><br>
      <script type ="text/javascript" src ="../scripts/{{cipher}}.js"></script>
    </div><br><br><br>
    {{/each}}



{{{messages_raw}}}


